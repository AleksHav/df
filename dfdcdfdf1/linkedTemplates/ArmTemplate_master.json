{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfdcdfdf1"
		},
		"geweAzureMySql1fgweg_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'geweAzureMySql1fgweg'"
		},
		"CosmosDb1g_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb1g'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfdcdfdf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"geweAzureMySql1fgweg_connectionString": {
						"value": "[parameters('geweAzureMySql1fgweg_connectionString')]"
					},
					"CosmosDb1g_connectionString": {
						"value": "[parameters('CosmosDb1g_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}